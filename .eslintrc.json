{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "jest",
    "@typescript-eslint"
  ],
  "extends": [
    "react-app",
    "plugin:jest/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "overrides": [
    {
      "files": [
        "*.mdx"
      ],
      "extends": [
        "plugin:mdx/recommended"
      ]
    }
  ]
}