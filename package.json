{
  "name": "johan-mouchet-react",
  "version": "5.0.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.5.1",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*.js",
    "lint:js": "eslint src/**/*.{js,jsx,json}",
    "lint:css": "stylelint src/**/*.{css,scss}",
    "format": "eslint --fix src/**/*.{js,jsx,json} && stylelint --fix src/**/*.{css,scss} && prettier --write src/**/*.md",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "_DISABLED_src/**/*.test.js": [
      "jest --bail --coverage --findRelatedTests",
      "eslint --fix",
      "git add"
    ],
    "_DISABLED_src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ],
    "_DISABLED_src/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "source-map-explorer": "^2.1.0",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.11.1"
  }
}
