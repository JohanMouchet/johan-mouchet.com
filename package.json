{
  "name": "johan-mouchet-react",
  "version": "5.3.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "apollo-link-prismic": "^1.0.8",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "dompurify": "^2.2.0",
    "focus-trap-react": "^7.0.1",
    "graphql": "^15.4.0",
    "html-react-parser": "^0.10.5",
    "node-sass": "^4.14.1",
    "prismic-reactjs": "^1.3.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.4",
    "wouter": "^2.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test",
    "type-check": "tsc --noEmit",
    "lint:js": "eslint src/**/*.{ts,tsx,js,json,mdx}",
    "lint:css": "stylelint src/**/*.scss",
    "analyze": "npm run build && source-map-explorer build/static/js/*.js",
    "format": "npm run lint:js -- --fix && npm run lint:css -- --fix && prettier --write src/**/*.md",
    "build": "react-scripts build",
    "build:storybook": "build-storybook -c .storybook -o build/storybook",
    "build:all": "react-scripts build && npm run build:storybook",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "[disabled]src/**/*.test.{ts,tsx,js}": [
      "npm run test"
    ],
    "src/**/*.{ts,tsx,js,json,mdx}": [
      "eslint --fix"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ],
    "src/**/*.md": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-viewport": "^6.3.6",
    "@storybook/addons": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^10.4.9",
    "@types/classnames": "^2.2.11",
    "@types/dompurify": "^2.0.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "source-map-explorer": "^2.5.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "typescript": "^3.9.7"
  }
}
