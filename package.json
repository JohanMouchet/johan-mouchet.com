{
  "name": "johan-mouchet",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint:js": "next lint",
    "lint:css": "stylelint src/**/*.scss",
    "format": "pnpm lint:js -- --fix && pnpm lint:css -- --fix && prettier --write src/**/*.{ts,tsx,js,json,mdx}",
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "build:storybook": "storybook build",
    "build:all": "pnpm build && pnpm build:storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint",
      "prettier --write"
    ],
    "*.scss": [
      "stylelint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@prismicio/client": "^6.7.3",
    "@prismicio/helpers": "^2.3.9",
    "@prismicio/react": "^2.5.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "dompurify": "^3.0.1",
    "fluxgrid": "^5.1.0",
    "html-react-parser": "^3.0.12",
    "isomorphic-dompurify": "^1.1.0",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.6",
    "@prismicio/types": "^0.2.7",
    "@storybook/addon-a11y": "^7.5.1",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/nextjs": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.7",
    "sass": "^1.59.2",
    "storybook": "^7.5.1",
    "stylelint": "^15.6.2",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^9.0.0"
  }
}
