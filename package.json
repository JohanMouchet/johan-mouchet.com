{
  "name": "johan-mouchet-react",
  "version": "5.2.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.0.2",
    "apollo-link-prismic": "^1.0.7",
    "classnames": "^2.2.6",
    "date-fns": "^2.11.1",
    "dompurify": "^2.0.12",
    "graphql": "^15.3.0",
    "html-react-parser": "^0.10.3",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "wouter": "^2.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:all": "react-scripts build && npm run build:storybook",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*.js",
    "type-check": "tsc --noEmit",
    "lint:js": "eslint src/**/*.{ts,tsx,js,json,mdx}",
    "lint:css": "stylelint src/**/*.scss",
    "format": "npm run lint:js -- --fix && npm run lint:css -- --fix && prettier --write src/**/*.md",
    "storybook": "start-storybook -p 9009 -s public",
    "build:storybook": "build-storybook -c .storybook -o build/storybook"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "[disabled]src/**/*.test.{ts,tsx,js}": [
      "npm run test"
    ],
    "src/**/*.{ts,tsx,js,json,mdx}": [
      "eslint --fix"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ],
    "src/**/*.md": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@types/classnames": "^2.2.10",
    "@types/dompurify": "^2.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-mdx": "^1.6.9",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.17.2",
    "typescript": "^3.9.5"
  }
}
